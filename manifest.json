{
  "manifest_version": 2,
  "name": "Tweet Quoter",
  "version": "1.0",
  "permissions": ["contextMenus", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
